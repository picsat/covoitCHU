{
    "name": "picsat/slim-covoitCHU",
    "description": "A Slim Framework App for manage a carpool CHU workers's calendar based on Slim-Skeleton",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "https://github.com/picsat/covoitCHU",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas SAN ANDRES",
            "email": "contact@picsat.fr",
            "homepage": "https://www.picsat.fr/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/database": "~5.1",
        "monolog/monolog": "^1.17",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.3",
        "slim/flash": "^0.1.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.5",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload" : {
        "psr-4" : {
            "App\\" : "app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
